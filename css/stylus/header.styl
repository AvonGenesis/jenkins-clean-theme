// Hide Jenkins text logo on header
#jenkins-name-icon
  display: none;

// Hide Jenkins head logo
#jenkins-head-icon
  opacity: 0;

// Not sure what this is for yet
.logo
  margin-bottom: 15px;

// Use our own picture for the header
#jenkins-home-link
  display: block;
  background: url("hawk.png") 0 0 no-repeat;
  height: 64px;
  width: 354px;

// Resize breadcrumb bar into a sexy header
#breadcrumbBar
  background-color: #39c !important;
  display: block;
  height: 60px!important;

// Be gone, evil breadcrumbs
#breadcrumbs
  display: none;
